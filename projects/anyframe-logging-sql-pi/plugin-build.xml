<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="logging-sql" description="Logging SQL Plugin consists of reference libraries needed for logging SQL using log4jdbc.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-logging-sql-pi</artifactId>
  <version>1.0.2</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;*"/>
  </dependent-plugins>
  <resources/>
  <build>
    <filesets>
      <fileset dir="src/main/resources" filtered="true" packaged="false">
        <include name="**/log4j.xml"/>
      </fileset>
    </filesets>
  </build>
  <interceptor>
    <class></class>
  </interceptor>
  <samples>false</samples>
  <essential>false</essential>
  <checked>false</checked>
</plugin>